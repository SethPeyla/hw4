<h1>Login</h1>

<%= form_with :url => "/sessions" do |form| %>
 <p>
  <%= form.label "first_name" %>
  <%= form.text_field "first_name" %>
  <%= form.label "last_name" %>
  <%= form.text_field "last_name" %>
  <%= form.label "email" %>
  <%= form.text_field "email" %>
  <%= form.label "password" %>
  <%= form.text_field "password" %>
</p>
  <%= form.submit "Login" %>
<% end %>